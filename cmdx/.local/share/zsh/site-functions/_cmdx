(( $+functions[_cmdx__run_commands] )) ||
_cmdx__run_commands() {
    local commands; commands=()
    _describe -t commands 'cmdx run commands' commands "$@"
}
(( $+functions[_cmdx__show_commands] )) ||
_cmdx__show_commands() {
    local commands; commands=()
    _describe -t commands 'cmdx show commands' commands "$@"
}

if [ "$funcstack[1]" = "_cmdx" ]; then
    _cmdx "$@"
else
    compdef _cmdx cmdx
fi
