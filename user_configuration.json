{
  "archinstall-language": "English",
  "additional-repositories": ["multilib"],
  "audio_config": {
    "audio": "pipewire"
  },
  "bootloader_config": {
    "bootloader": "Systemd-boot",
    "uki": false,
    "removable": false
  },
  "debug": false,
  "disk_config": {
    "config_type": "default_layout",
    "device_modifications": [
      {
        "device": "/dev/nvme0n1",
        "partitions": [
          {
            "btrfs": [],
            "flags": ["boot"],
            "fs_type": "fat32",
            "size": {
              "sector_size": null,
              "unit": "MiB",
              "value": 1024
            },
            "mount_options": [],
            "mountpoint": "/boot",
            "start": {
              "sector_size": null,
              "unit": "MiB",
              "value": 1
            },
            "status": "create",
            "type": "primary"
          },
          {
            "btrfs": [
              { "mountpoint": "/", "name": "@" },
              { "mountpoint": "/home", "name": "@home" },
              { "mountpoint": "/var", "name": "@var" },
              { "mountpoint": "/.snapshots", "name": "@snapshots" }
            ],
            "flags": [],
            "fs_type": "btrfs",
            "size": {
              "sector_size": null,
              "unit": "Percent",
              "value": 100
            },
            "mount_options": ["compress=zstd", "noatime"],
            "mountpoint": null,
            "start": {
              "sector_size": null,
              "unit": "MiB",
              "value": 1025
            },
            "status": "create",
            "type": "primary"
          }
        ],
        "wipe": true
      }
    ]
  },
  "hostname": "archlinux",
  "kernels": ["linux"],
  "locale_config": {
    "kb_layout": "us",
    "sys_enc": "UTF-8",
    "sys_lang": "en_US"
  },
  "network_config": {
    "type": "nm"
  },
  "no_pkg_lookups": false,
  "ntp": true,
  "offline": false,
  "packages": [
    "git",
    "linux-headers",
    "base-devel",
    "stow",
    "bluez",
    "bluez-utils"
  ],
  "parallel downloads": 5,
  "profile_config": {
    "profile": {
      "main": "Minimal"
    }
  },
  "swap": {
    "enabled": true,
    "algorithm": "zstd"
  },
  "timezone": "Asia/Kolkata"
}
